package exer;

public class NotasArray {
	public static void main(String[] args) {
		
		double [] notas = new double[3];
		
		notas[0] = 5.6;
		notas[1] = 6.9;
		notas[2] = 9.8;
		double mediaNota = 0;
		
		for(int i = 0; i < notas.length; i++) {
			System.out.println("Nota: " + notas[i]);
			
			mediaNota += notas[i];
		}
		
		System.out.println(mediaNota / notas.length);
	}
}
